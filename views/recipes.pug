extends layout

block content
    .container.py-5.d-flex.justify-content-center.flex-wrap
        each recipe in recipeData  
            .card(style='width: 18rem;').m-3
                img.card-img-top.p-3.rounded(src=recipe.photoUrl alt='')
                .card-body
                    h5.card-title #{recipe.title}
                    p.card-text #{recipe.category}
                    a.btn.btn-primary(href='#') Zum Rezept
